export PATH=./bin:$HOME/.dotfiles/bin:/usr/local/bin:/usr/local/sbin:$PATH:$HOME/.rvm/bin:$HOME/.composer/vendor/bin

export EDITOR=nvim

export LC_CTYPE=en_US.UTF-8
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8
export DISABLE_UPDATE_PROMPT=true

export VAGRANT_DEFAULT_PROVIDER=vmware_fusion

export AWS_DEFAULT_PROFILE=zippy
export AWS_DEFAULT_REGION=ap-southeast-2

export NVM_DIR=~/.nvm
export NVM_LAZY_LOAD=true

export ANDROID_HOME=/usr/local/opt/android-sdk
export REACT_EDITOR=vim

export ANTIGEN_COMPDUMP=$ZDOTDIR/.zcompdump
export ANTIGEN_COMPDUMPFILE=$ZDOTDIR/.zcompdump
